<!DOCTYPE html>
<html lang="en">

<%- include standardnavbar.ejs %>

<head>
    <link rel="stylesheet" type="text/css" href="/css/tabs.css" />
    <link rel="stylesheet" type="text/css" href="/css/tabstylesadmin.css" />
    <meta name="_csrf" content="<%= csrfToken %>" />
    <!-- datepicker CSS -->
    <link href="/css/datepicker.css" rel="stylesheet">
</head>

<body>
    <div class="page-wrap static-background">
    <section class="container rcorners">
        <div class="tabs tabs-style-linemove">  
            <nav>
                <ul>
                    <li><a href="#profile"><i class="fa fa-user tabicon"></i><span>Users</span></a></li>
                    <li><a href="#courses"><i class="fa fa-list tabicon"></i><span>Courses</span></a></li>
                    <li><a href="#addcourses"><i class="fa fa-plus-square-o tabicon"></i><span>Add Courses</span></a></li>
                    <li><a href="#vol"><i class="fa fa-users tabicon"></i><span>Volunteers</span></a></li>
                    <li><a href="#rental"><i class="fa fa-square tabicon"></i><span>Rental View</span></a></li>
                    <li><a href="#report"><i class="fa fa-square tabicon"></i><span>Report View</span></a></li>
                </ul>
            </nav>
            <div class="content-wrap">
             
                <section id="profile">
                    <!-- Tab On-Click fills profile tab-->
                </section>
                <section id="courses">
                    <!-- Tab On-Click fills courses-->
                </section>
                <section id="addcourses">
                    <!-- On Load fills add courses -->
                </section>
                <section id="vol">Volunteers</section>
                <section id="rental">Rental</section>
                <section id="report">Report</section>
            </div><!-- /content -->
        </div><!-- /tabs -->
    </section>
            

    </div><!-- /container -->


    <!-- Tabs JavaScript -->
    <script src="/js/tabs.js"></script>

    <!-- Scroll to Top Button (Only visible on small and extra-small screen sizes) -->
    <div class="scroll-top page-scroll visible-xs visible-sm">
        <a class="btn btn-primary" href="#page-top">
            <i class="fa fa-chevron-up"></i>
        </a>
    </div>

    <!-- jQuery -->
    <script src="/js/jquery.js"></script>
    <script src="/js/bootstrap-datepicker.js"></script>
    <script src="/js/rangedatepicker.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/js/bootstrap.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <script src="/js/classie.js"></script>
    <script src="/js/cbpAnimatedHeader.js"></script>

    <!-- Contact Form JavaScript -->
    <script src="/js/jqBootstrapValidation.js"></script>
    <script src="/js/contact_me.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="/js/freelancer.js"></script>
    <script src="/js/adminajax.js"></script>
    <!-- Set Background Javascript -->
    <script src="/js/setBackgroundImage.js"></script>
</body>

<%- include standardfooter %>

</html>